.socials__menu {
  display: flex;
  margin-bottom: 40px;
}

.socials__item {
  margin-right: 20px;
  
  &:last-child {
    margin-right: 0;
  }
}

.socials__link {
  width: 46px;
  height: 46px;
  display: block;
  width: 46px;
  height: 46px;
  background-color: #e33861;
  border-radius: 50%;
  position: relative;
}

.socials__icon {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}